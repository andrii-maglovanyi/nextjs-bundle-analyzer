  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
